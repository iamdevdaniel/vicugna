{
  "expo": {
    "name": "vicugna",
    "slug": "vicugna",
    "version": "1.0.0",
	"icon": "./assets/images/vicugna-logo-day.png",
    "orientation": "portrait",
    "scheme": "vicugna",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.maydanachi.vicugna"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "@morrowdigital/watermelondb-expo-plugin",
        { "disableJsi": true }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so"
              ]
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a0de7245-1ed6-44cd-9495-86ecd499a78f"
      }
    },
    "owner": "maydanachi"
  }
}
